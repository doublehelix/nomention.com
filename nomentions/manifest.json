{
    "manifest_version": 2,
    "name": "NoMentions",
    "version": "1.0",
    "description": "Replaces all mentions of men with a pretty image.",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "jquery-3.1.1.slim.min.js",
                "nomentions.js"
            ]
        }
    ]
}